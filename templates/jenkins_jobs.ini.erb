[job_builder]
cmd_tries=12
cmd_sleep=10

[jenkins]
user=<%= @username %>
password=<%= @password %>
url=<%= @url %>

<% @extensions.each do |extension| -%>
["<%= extension['name'] %>"]
<% @extension['parameters'].each do |parameter| -%>
<%= parameter['name'] %> = <%= parameter['value'] %>
<% end -%>
<% end -%>
