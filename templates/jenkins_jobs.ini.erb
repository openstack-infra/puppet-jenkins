[job_builder]
cmd_tries=<%= @cmd_tries %>
cmd_sleep=<%= @cmd_sleep %>

[jenkins]
user=<%= @username %>
password=<%= @password %>
url=<%= @url %>

<% @extensions.each do |extension| -%>
["<%= extension['name'] %>"]
<% @extension['parameters'].each do |parameter| -%>
<%= parameter['name'] %> = <%= parameter['value'] %>
<% end -%>
<% end -%>
